{
    "name": "laminas-api-tools/api-tools-oauth2",
    "description": "Laminas module for implementing an OAuth2 server",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "oauth2",
        "framework"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-oauth2/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-oauth2",
        "rss": "https://github.com/laminas-api-tools/api-tools-oauth2/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "bshaffer/oauth2-server-php": "~1.3",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0",
        "laminas/laminas-crypt": "~2.3",
        "laminas/laminas-http": "~2.3",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-servicemanager": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "*@dev",
        "laminas/laminas-loader": "~2.3",
        "laminas/laminas-test": "~2.3",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\OAuth2\\": "src/",
            "LaminasTest\\ApiTools\\OAuth2\\": "test/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "replace": {
        "zfcampus/zf-oauth2": "~1.0.0.0"
    }
}
